<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="BaseRecyclerView">
        <attr name="brw_layout_manager" format="enum">
            <enum name="linear" value="0" />
            <enum name="grid" value="1" />
            <enum name="staggered" value="2" />
        </attr>
        <attr name="brw_orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="brw_reverse_layout" format="boolean" />
        <attr name="brw_span_count" format="integer" />
        <attr name="brw_stack_from_end" format="boolean" />
    </declare-styleable>

</resources>